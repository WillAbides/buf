{
  "version": "v1",
  "managed": {
    "enabled": true,
    "go_package_prefix": {
      "default": "${HOST_ADDR|localhost}/foo/bar/gen/go"
    }
  },
  "plugins": [
    {
      "name": "go",
      "out": "gen/go",
      "opt": "plugins=grpc",
      "path": "/path/to/foo",
      "strategy": "all"
    }
  ]
}
